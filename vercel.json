{
  "rootDirectory": "frontend",
  "buildCommand": "npm cache clean --force && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "frontend/index.html"
    }
  ],
  "env": {
    "NPM_CONFIG_PRODUCTION": "false"
  }
}
