{
  "buildCommand": "cd frontend && npm cache clean --force && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NPM_CONFIG_PRODUCTION": "false"
  }
}
